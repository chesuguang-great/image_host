{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "app.py"
    },
    {
      "src": "/images/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "CDN-Cache-Control": "public, max-age=31536000, immutable",
        "Access-Control-Allow-Origin": "https://www.suguang.site"
      },
      "dest": "https://raw.githubusercontent.com/<suguang-great>/<image-host>/main/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}